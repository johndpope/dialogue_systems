[{"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["WP", "IN", "PRP", "."], "qSentId": 0, "question": "Here.", "question_pos_tagged": ["RB", "."], "aSentId": 0, "answer": "What about you?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["NNP", "."], "qSentId": 1, "question": "I'm not hungry - leastways, not for lembas bread.", "question_pos_tagged": ["PRP", "VBP", "RB", "JJ", ":", "NNS", ",", "RB", "IN", "RB", "JJ", "."], "aSentId": 1, "answer": "Sam.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["IN", "WP", "."], "qSentId": 2, "question": "Alright. We don't have that much left.", "question_pos_tagged": ["NNP", "NNP", "VBP", "RB", "VB", "IN", "RB", "VBN", "."], "aSentId": 2, "answer": "For what?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "THEODEN", "speaker2": "EOWYN", "docId": 382, "answer_pos_tagged": ["NNP", "NNP", "NN", "."], "qSentId": 4, "question": "Tonight we remember those who gave their blood to defend this country. Hail the victorious dead!", "question_pos_tagged": ["NNP", "PRP", "VBP", "DT", "WP", "VBP", "PRP$", "NN", "TO", "VB", "DT", "NNP", "NNP", "DT", "JJ", "NN", "."], "aSentId": 4, "answer": "Westu Aragorn hal.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"LOCATION": ["Aragorn"]}, "question_ne_tagged": {}}, {"speaker1": "THEODEN", "speaker2": "EOWYN", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "DT", "JJ", "NNS", "."], "qSentId": 5, "question": "I am happy for you.", "question_pos_tagged": ["PRP", "VBP", "JJ", "IN", "PRP", "."], "aSentId": 5, "answer": "You are both honourable men.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["DT", "NN", ":", "NN", "."], "qSentId": 7, "question": "No news of Frodo?", "question_pos_tagged": ["DT", "NN", "IN", "NNP", "."], "aSentId": 7, "answer": "No word ... nothing.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"LOCATION": ["Frodo"]}}, {"speaker1": "ARAGORN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["NNP", "PRP", "VB", "IN", "."], "qSentId": 8, "question": "We till have time.", "question_pos_tagged": ["PRP", "VBP", "VBP", "NN", "."], "aSentId": 8, "answer": "Do we know that?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["DT", "NNP", "VBZ", "NNP", "NNP", ":", "NNS", ",", "PRP", "VBZ", "JJ", "."], "qSentId": 9, "question": "What does your heart tell you?", "question_pos_tagged": ["WP", "VBZ", "PRP$", "NN", "NN", "PRP", "."], "aSentId": 9, "answer": "That Frodo is alive. Yes - yes, he is alive.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SMEAGOL", "speaker2": "GOLLUM", "docId": 382, "answer_pos_tagged": ["NNP", ":", "NNP", "."], "qSentId": 10, "question": "Too risky, too risky. The thieves! They stole it from us. Kill them . . . kill them ... kill them both!", "question_pos_tagged": ["RB", "JJ", ",", "RB", "NNP", "NNP", "NNS", ".", "PRP", "VBP", "PRP", "IN", "NNP", "NNP", "PRP", ".", ".", ".", "NN", "PRP", ":", "VBP", "PRP", "DT", "."], "aSentId": 10, "answer": "Shhh! Quiet!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SMEAGOL", "speaker2": "GOLLUM", "docId": 382, "answer_pos_tagged": ["CC", "PRP", "MD", ".", ".", ",", ":", "NNP", "VBD", "PRP", "NNP", ".", ".", ".", ".", ".", "PRP", "MD", "VB", "PRP", "RB", "."], "qSentId": 11, "question": "They knows, they knows, they suspects us.", "question_pos_tagged": ["PRP", "VBZ", ",", "PRP", "VBZ", ",", "PRP", "VBZ", "PRP", "."], "aSentId": 11, "answer": "And we will . . ,- Smeagol did it once. . . . . . He can do it again.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"PERSON": ["Smeagol"]}, "question_ne_tagged": {}}, {"speaker1": "SMEAGOL", "speaker2": "GOLLUM", "docId": 382, "answer_pos_tagged": ["NN", ",", "NN", ",", "PRP$", "NNP", "NNP", "PRP", "MD", "VB", "PRP", "TO", "PRP$", "."], "qSentId": 12, "question": "We must get the Precious. We must get it back.", "question_pos_tagged": ["PRP", "MD", "VB", "DT", "NNP", "NNP", "MD", "VB", "PRP", "RB", "."], "aSentId": 12, "answer": "Patience, patience, my love. First we must lead them to her.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SMEAGOL", "speaker2": "GOLLUM", "docId": 382, "answer_pos_tagged": ["NNS", ",", "DT", "NNS", ":", "CC", "RB", "."], "qSentId": 13, "question": "We lead them to the windy stairs.", "question_pos_tagged": ["PRP", "VBP", "PRP", "TO", "DT", "NN", "NNS", "."], "aSentId": 13, "answer": "Yes, the stairs ... and then?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SMEAGOL", "speaker2": "GOLLUM", "docId": 382, "answer_pos_tagged": ["CC", "WRB", "PRP", "VBP", "IN", ",", "EX", "VBZ", "DT", "VBG", "NNP", "NNP", "POS", "RB", "JJ", ",", "PRP", "RB", "VBZ", "TO", "NNP", "NNP", "MD", "VB", ",", "DT", "NNP", "VBZ", "VBZ", "JJ", "NNS", "."], "qSentId": 14, "question": "Up, up, up, up the stairs we go . . . until we come to . . . ... the Tunnel!", "question_pos_tagged": ["IN", ",", "RB", ",", "RB", ",", "RB", "DT", "NNS", "PRP", "VBP", ".", ".", ".", "IN", "PRP", "VBP", "TO", ".", ".", ".", ":", "DT", "NNP", "."], "aSentId": 14, "answer": "And when they go in, there's no coming out. She's always hungry, she always needs to feed. She must eat, all She gets is filthy Orcses.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SMEAGOL", "speaker2": "GOLLUM", "docId": 382, "answer_pos_tagged": ["DT", ".", ".", ".", "RB", "RB", "JJ", "IN", "DT", ",", "PRP$", "NNP", "NNP", "NNS", "IN", "NN", "NNS", ".", ".", "."], "qSentId": 15, "question": "And they doesn't taste very nice, does they, Precious? .", "question_pos_tagged": ["CC", "PRP", "VBZ", "RB", "VB", "RB", "JJ", ",", "VBZ", "PRP", ",", "NNP", ".", "."], "aSentId": 15, "answer": "No . . . not very nice at all, my love. She hungers for sweeter meats . . .", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SMEAGOL", "speaker2": "GOLLUM", "docId": 382, "answer_pos_tagged": ["IN", "PRP", ".", ".", "."], "qSentId": 16, "question": "And take it for Me!", "question_pos_tagged": ["CC", "VB", "PRP", "IN", "PRP", "."], "aSentId": 16, "answer": "For us . . .", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SMEAGOL", "speaker2": "GOLLUM", "docId": 382, "answer_pos_tagged": ["DT", "NNP", "MD", "VB", "NNS", "RB", "DT", "NNP", "VBP", "JJ", "."], "qSentId": 17, "question": "Yes, we, we meant for us . . . Go Hum! Go Hum!", "question_pos_tagged": ["NNS", ",", "PRP", ",", "PRP", "VBD", "IN", "PRP", ".", ".", ".", "NNP", "NNP", ".", "NNP", "NNP", "."], "aSentId": 17, "answer": "The Precious will be ours once the Hobbitses are dead!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["NNP", ".", ".", "DT"], "qSentId": 18, "question": "You miserable little maggot! I'll stove your head in!", "question_pos_tagged": ["PRP", "VBP", "JJ", "NN", ".", "PRP", "MD", "VB", "PRP$", "NN", "IN", "."], "aSentId": 18, "answer": "Sm. . . a", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["NNP", ":", "IN", "PRP", "VBP", "PRP", "RP", ",", "PRP", "VBP", "VBN", "."], "qSentId": 19, "question": "Call me a liar! You're a liar!", "question_pos_tagged": ["NNP", "PRP", "DT", "NN", ".", "PRP", "VBP", "DT", "NN", "."], "aSentId": 19, "answer": "Sam! If you scare him off, we're lost.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "RB", "VBG", "PRP", "RB", "."], "qSentId": 20, "question": "I don't care! I can't do it, Mr Frodo. I won' t wait around for him to kill us !", "question_pos_tagged": ["PRP", "VBP", "RB", "VB", ".", "PRP", "MD", "RB", "VB", "PRP", ",", "NNP", "NNP", "NNP", "VBD", "POS", "NN", "NN", "IN", "IN", "PRP", "TO", "VB", "PRP", "."], "aSentId": 20, "answer": "I'm not sending him away.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"PERSON": ["Mr Frodo"]}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["PRP", "MD", "RB", "VB", "DT", "IN", "NNS", ",", "NNP", "NNP", "IN", "DT", "NNP", "NNP", "VBP", "PRP", "IN", "PRP$", "NN", "."], "qSentId": 21, "question": "You don't see it, do you? He's a villain!", "question_pos_tagged": ["PRP", "VBP", "RB", "VB", "PRP", ",", "VBP", "PRP", ".", "PRP", "VBZ", "DT", "NN", "."], "aSentId": 21, "answer": "We can't do this by ourselves, Sam. Not without a guide. I need you on my side.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"PERSON": ["Sam"]}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", ",", "NNP", ",", "PRP", "NNP", "NNP", "MD", "VB", "PRP", "."], "qSentId": 22, "question": "I'm on your side, Mr Frodo.", "question_pos_tagged": ["PRP", "VBP", "IN", "PRP$", "NN", ",", "NNP", "NNP", "."], "aSentId": 22, "answer": "I know, Sam, I know. You must trust me.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"PERSON": ["Sam"]}, "question_ne_tagged": {"PERSON": ["Mr Frodo"]}}, {"speaker1": "PIPPIN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["NNP", "NNP", ".", ".", ".", "VBZ", "IN", "WP", "PRP", "VBD", "."], "qSentId": 23, "question": "Gandalf! Forgive me. A tree ... There was a white tree . . . in a courtyard of stone . . . It was dead!", "question_pos_tagged": ["NNP", ":", "NNP", "NNP", "NNP", "NN", ":", "EX", "VBD", "DT", "JJ", "NN", ".", ".", ".", "IN", "DT", "NN", "IN", "NN", ".", ".", ".", "PRP", "VBD", "JJ", "."], "aSentId": 23, "answer": "Minas Tirith??? Is that what you saw?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"ORGANIZATION": ["Minas Tirith"]}, "question_ne_tagged": {}}, {"speaker1": "PIPPIN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["CC", "WP", "VBD", "PRP", "VBP", "PRP", ".", "NNP", "."], "qSentId": 24, "question": "I saw . . . I saw him . . . I could hear his voice in my head.", "question_pos_tagged": ["PRP", "VBD", ".", ".", ".", "PRP", "VBD", "PRP", ".", ".", ".", "PRP", "MD", "VB", "PRP$", "NN", "IN", "PRP$", "NN", "."], "aSentId": 24, "answer": "And what did you tell him? Speak!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "PIPPIN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["WP", "VBD", "PRP", "VBP", "PRP", "IN", "NNP", "CC", "DT", "NNP", ".", "EX", "VBD", "DT", "NN", "IN", "NNP", "POS", "NNS", ":", "DT", "NN", ",", "CC", "DT", "NN", "NN", "PRP", "NNP", "NNP", "VBD", "NNP", "NN", "IN", "NNP", "CC", "DT", "NNP", "."], "qSentId": 25, "question": "He asked me my name, I didn't answer . . . he hurt me . . .", "question_pos_tagged": ["PRP", "VBD", "PRP", "PRP$", "NN", ",", "PRP", "VBD", "RB", "VB", ".", ".", ".", "PRP", "VBD", "PRP", ".", ".", "."], "aSentId": 25, "answer": "What did you tell him about Frodo and the Ring? There was no lie in Pippin's eyes; a fool, but an honest fool he remains. He told Sauron nothing of Frodo and the Ring.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"PERSON": ["Frodo", "Sauron", "Frodo"], "LOCATION": ["Pippin"]}, "question_ne_tagged": {}}, {"speaker1": "PIPPIN", "speaker2": "MERRY", "docId": 382, "answer_pos_tagged": ["WRB", "VBD", "PRP", "VBP", ":", "WRB", "VBP", "PRP", "RB", "VBP", "TO", "VB", ".", "PRP", "RB", "MD", "."], "qSentId": 26, "question": "Where are we going?", "question_pos_tagged": ["WRB", "VBP", "PRP", "VBG", "."], "aSentId": 26, "answer": "Why did you look - why do you always have to look? You never can.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "PIPPIN", "speaker2": "MERRY", "docId": 382, "answer_pos_tagged": ["NNP", "RB", "PRP", "VBP", ":", "DT", "NNP", "VBZ", "PRP", "VBP", "DT", "NNP", ".", ".", ".", "PRP", "VBZ", "VBG", "TO", "VB", "VBG", "IN", "PRP", ",", "NNP", "NNP", "VBP", "TO", "VB", "PRP", "RP", "IN", "RB", "."], "qSentId": 27, "question": ". . . . ,.;  I'm sorry, alright? I won't do it again.", "question_pos_tagged": [".", ".", ".", ".", ",", ".", ":", "PRP", "VBP", "JJ", ",", "NN", ".", "PRP", "MD", "RB", "VB", "PRP", "RB", "."], "aSentId": 27, "answer": "Don't you understand ... the Enemy thinks you have the Ring . . . he's going to be looking for you, Pip. They have to get you out of here!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "PIPPIN", "speaker2": "MERRY", "docId": 382, "answer_pos_tagged": ["NNP", "IN", "."], "qSentId": 28, "question": "And you. You're coming with me . . . Merry?", "question_pos_tagged": ["CC", "NNP", "NNP", "NNP", "VBG", "IN", "PRP", ".", ".", ".", "NNP", "."], "aSentId": 28, "answer": "Come on!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "MERRY", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["DT", "JJ", "IN", "DT", "NNP", "NNP", "."], "qSentId": 30, "question": "Here - something for the road.", "question_pos_tagged": ["RB", ":", "NN", "IN", "DT", "NN", "."], "aSentId": 30, "answer": "The last of the Longbottom Leaf.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "MERRY", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["CC", "-NONE-", "VBP", "DT", "JJ", "RB", "."], "qSentId": 31, "question": "I know you've run out . . . you smoke too much, Pip.", "question_pos_tagged": ["PRP", "VBP", "PRP", "VBP", "VBN", "RP", ".", ".", ".", "PRP", "VBD", "RB", "RB", ",", "NNP", "."], "aSentId": 31, "answer": "But we'11 see each other soon?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "MERRY", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["NNP", "."], "qSentId": 32, "question": "I don't know . . . I don't know what's going to happen.", "question_pos_tagged": ["PRP", "VBP", "RB", "VB", ".", ".", ".", "PRP", "VBP", "RB", "VB", "WP", "POS", "VBG", "TO", "VB", "."], "aSentId": 32, "answer": "Merry?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ELROND", "speaker2": "ARWEN", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "DT", "NN", "IN", "NN", ":", "NN", "PRP", "WP", "PRP", "VBP", "VBN", "."], "qSentId": 33, "question": "Arwen?", "question_pos_tagged": ["NNP", "."], "aSentId": 33, "answer": "You have the gift of foresight - tell me what you have seen.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ELROND", "speaker2": "ARWEN", "docId": 382, "answer_pos_tagged": ["EX", "VBZ", "RB", "NNP", ","], "qSentId": 34, "question": "I looked into your future and I saw death.", "question_pos_tagged": ["PRP", "VBD", "IN", "PRP$", "JJ", "CC", "PRP", "VBD", "NN", "."], "aSentId": 34, "answer": "There is also life. ,", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ELROND", "speaker2": "ARWEN", "docId": 382, "answer_pos_tagged": ["CC", "PRP", "VBZ", "RB", "VBN", ":"], "qSentId": 35, "question": "That future is almost gone.", "question_pos_tagged": ["DT", "JJ", "VBZ", "RB", "VBN", "."], "aSentId": 35, "answer": "But it is not lost ...", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ELROND", "speaker2": "ARWEN", "docId": 382, "answer_pos_tagged": ["DT", "NNS", "VBP", "NNP", "NNP", "NNP", "VBP", "PRP", "RB", "PRP", "MD", "VB", "PRP", "NNP", "NNP", ",", "PRP", "VBZ", "NNP", "NNP", ":"], "qSentId": 36, "question": "Nothing is certain.", "question_pos_tagged": ["NN", "VBZ", "JJ", "."], "aSentId": 36, "answer": "Some things are certain. If I leave him now I will regret It forever. Ada, it is time. Ada ...", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ELROND", "speaker2": "ARWEN", "docId": 382, "answer_pos_tagged": ["DT", "VBD", "PRP$", "NNP", "NNP", ":", "NNP", "IN", "PRP$", "MD", "CC", "RB", ",", "EX", "VBZ", "DT", "NN", "RB", "IN", "MD", "VB", "PRP", "NN", "."], "qSentId": 37, "question": "Your hands are cold. The life of the Eldar is leaving you.", "question_pos_tagged": ["PRP$", "NNS", "VBP", "NNP", "NNP", "NN", "IN", "DT", "NNP", "VBZ", "VBG", "PRP", "."], "aSentId": 37, "answer": "This was my choice. Ada ... Whether by your will or not, there is no ship now that can bear me hence.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "GANDALF", "speaker2": "DENETHOR", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "PRP", "VBP", "RB", ",", "NNP", ",", "RB", "IN", "DT", "PRP$", "PRP", "VBP", "RB", "NNP", "NNP", "PRP", "VBP", "DT", "NNS", "IN", "DT", "NNP", "NNP", "VBP", "JJ", ".", "PRP", "VBP", "VBN", "RBR", "IN", "PRP", "NNP", "NNP", "PRP$", "NN", "NN", "PRP", "MD", "VB", "PRP", "IN", "DT", "NN", "IN", "NNP", "CC", "IN", "PRP$", "NN", "PRP", "MD", "VB", "TO", "VB", "PRP", "."], "qSentId": 38, "question": "Get up! My Lord, there will be a time to grieve for Boromir, but it is not now. War is coming ... the Enemy is on your doorstep. As Steward, you are charged with the defence of this city. Where are Condor's armies?", "question_pos_tagged": ["NN", "IN", ".", "NNP", "NNP", ",", "EX", "MD", "VB", "DT", "NN", "TO", "VB", "IN", "NNP", ",", "CC", "PRP", "VBZ", "RB", "NNP", "NNP", "VBZ", "VBG", ":", "DT", "NNP", "VBZ", "IN", "PRP$", "NNP", "NNP", "NNP", ",", "PRP", "VBP", "VBN", "IN", "DT", "NN", "IN", "DT", "NNP", "NNP", "VBP", "NNP", "POS", "NNS", "."], "aSentId": 38, "answer": "You think you are wise, Mithrandir, yet for all your subtleties.you have not wisdom. Do you think the eyes of the White Tower are blind? I have seen more than you know. With your left hand you would use me as a shield against Mordor and with your right you would seek to supplant me.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"PERSON": ["Mithrandir"]}, "question_ne_tagged": {"ORGANIZATION": ["Condor"]}}, {"speaker1": "GANDALF", "speaker2": "DENETHOR", "docId": 382, "answer_pos_tagged": ["DT", "NN", "IN", "NNP", "VBZ", "JJ", ",", "CC", "DT", "NNS", "."], "qSentId": 39, "question": "Authority is not given to you to deny the return of the King - Steward!", "question_pos_tagged": ["NN", "VBZ", "RB", "VBN", "TO", "PRP", "TO", "VB", "DT", "NN", "IN", "DT", "NNP", ":", "NNP", "."], "aSentId": 39, "answer": "The rule of Condor is mine, and no others!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "GANDALF", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["IN", ",", "PRP", "VBP", "DT", "VBZ", "RB", "DT", "JJ", "NN", ".", ".", ".", "PRP", "VBP", ",", "PRP", "VBP", "RB", "RB", "VB", "PRP", "TO", "VB", "DT", "VBG", ".", ".", ".", "VBP", "PRP", "."], "qSentId": 41, "question": "Come.", "question_pos_tagged": ["NNP", "."], "aSentId": 41, "answer": "So, I imagine this is just a ceremonial position . . . I mean, they don't actually expect me to do any fighting . . . do they?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "GANDALF", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["PRP", "VBZ", "RB", "JJ", "."], "qSentId": 42, "question": "You're in the service of the Steward now - you're going to have to do as you're told, Peregrin Took . . . Guard of the Citadel!", "question_pos_tagged": ["PRP", "VBP", "IN", "DT", "NN", "IN", "DT", "NNP", "RB", ":", "PRP", "VBP", "VBG", "TO", "VB", "TO", "VB", "RB", "PRP", "VBP", "VBN", ",", "NNP", "NNP", ".", ".", ".", "NNP", "IN", "DT", "NNP", "."], "aSentId": 42, "answer": "It's so quiet.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"ORGANIZATION": ["Steward", "Citadel!"]}}, {"speaker1": "GANDALF", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "RB", "VB", "TO", "VB", "IN", "DT", "NN", ".", ".", ".", "CC", "VBG", "IN", "DT", "NN", "IN", "CD", "PRP", "MD", "RB", "VB", "VBZ", "RB", "NNP", "NNP"], "qSentId": 43, "question": "It's the deep breath before the plunge.", "question_pos_tagged": ["PRP", "VBZ", "DT", "NN", "NN", "IN", "DT", "NN", "."], "aSentId": 43, "answer": "I don't want to be in a battle . . . but waiting on the edge of one I can't escape is even worse. L", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "GANDALF", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["CC", "PRP", "VBP", "DT", "NNP", "NNP", "NNP", "POS", "NN", "TO", "VB", "IN", "NN", "."], "qSentId": 44, "question": "There never was much hope. This will be the end of Gondor as we know it. Here the hammerstroke will fall hardest.", "question_pos_tagged": ["EX", "RB", "VBD", "RB", "NNP", "NNP", "MD", "VB", "DT", "NN", "IN", "NNP", "IN", "PRP", "VBP", "NNP", "NNP", "DT", "NN", "MD", "VB", "JJS", "."], "aSentId": 44, "answer": "But we have the White Wizard. That's got to count for something.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"ORGANIZATION": ["White Wizard"]}, "question_ne_tagged": {}}, {"speaker1": "DENETHOR", "speaker2": "FARAMIR", "docId": 382, "answer_pos_tagged": ["WP", "MD", "PRP", "VB", "PRP", "VBP", "."], "qSentId": 46, "question": "And I shall not forget it, nor fail to reward that which is given ...", "question_pos_tagged": ["CC", "PRP", "MD", "RB", "VB", "PRP", ",", "CC", "NN", "TO", "VB", "IN", "WDT", "VBZ", "VBN", ":"], "aSentId": 46, "answer": "What would you have me do?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "DENETHOR", "speaker2": "FARAMIR", "docId": 382, "answer_pos_tagged": ["PRP$", "NNP", ",", "NNP", "VBZ", "VBN", "."], "qSentId": 47, "question": "I will not yield the River and Pelennor unfought - Osgiliath must be retaken.", "question_pos_tagged": ["PRP", "MD", "RB", "VB", "DT", "NNP", "CC", "NNP", "NN", ":", "NNP", "MD", "VB", "VBN", "."], "aSentId": 47, "answer": "My Lord, Osgiliath is overrun.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "DENETHOR", "speaker2": "FARAMIR", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "RB", "IN", "PRP$", "NNS", "VBD", "VBN", "VBN", ",", "IN", "PRP", "VBD", "VBN", "CC", "NNP", "VBD", "VBN", "."], "qSentId": 48, "question": "Much must be risked in war. Is there a Captain here who still has the courage to do his Lord's will?", "question_pos_tagged": ["RB", "MD", "VB", "VBN", "IN", "NNP", "NNP", "RB", "DT", "NNP", "RB", "WP", "RB", "VBZ", "DT", "NN", "TO", "VB", "PRP$", "NNP", "POS", "MD", "."], "aSentId": 48, "answer": "You wish now that our places had been exchanged, that I had died and Boromir had lived?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "DENETHOR", "speaker2": "FARAMIR", "docId": 382, "answer_pos_tagged": ["IN", "PRP", "VBP", "VBN", "IN", "NNP", ",", "PRP", "MD", "VB", "WP", "PRP", "MD", "IN", "PRP$", "NN", "."], "qSentId": 49, "question": "Yes, I wish that.", "question_pos_tagged": ["NNS", ",", "PRP", "VBP", "IN", "."], "aSentId": 49, "answer": "Since you are robbed of Boromir, I will do what I can in his stead.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "SMEAGOL", "docId": 382, "answer_pos_tagged": ["VBG", ".", "NNP", ".", "NNP", "NNP", "VBZ", "RB", "RB", "NNP", "NNP", "VBZ", "PRP", "VBP", "NNS", "IN", "NN", "RB", "MD", "VB", "CC", "PRP", "VBP", "``", "NNP", ".", "''", ",", "``", "NNP", ".", "''", ".", "NNP", "NN", "NNS", ",", "NN", "NNS", ",", "PRP$", "NNP", ",", "RB", "JJ", "."], "qSentId": 51, "question": "What are you up to? Sneaking off, are we?", "question_pos_tagged": ["WP", "VBP", "PRP", "IN", "TO", ".", "NNP", "IN", ",", "VBP", "PRP", "."], "aSentId": 51, "answer": "Sneaking? Sneaking? Fat Hobbit is always so polite. Smeagol shows them secret ways that nobody else could find and they say \"Sneak!\", \"Sneak!\". Very nice friends, oh yes, my Precious, very nice.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "SMEAGOL", "docId": 382, "answer_pos_tagged": ["VBG", "``"], "qSentId": 52, "question": "Alright, alright - you just startled me is all. What were you doing?", "question_pos_tagged": ["NNP", ",", "VBD", ":", "PRP", "RB", "VBD", "PRP", "VBZ", "NNP", "WP", "VBD", "PRP", "VBG", "."], "aSentId": 52, "answer": "Sneaking!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["NNP", ":", "NNP", "PRP", "."], "qSentId": 54, "question": "That's a filthy lie!", "question_pos_tagged": ["DT", "VBZ", "DT", "JJ", "NN", "."], "aSentId": 54, "answer": "Sam! Stop it!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["DT", ",", "NNP", "."], "qSentId": 55, "question": "I'll kill him! I'll kill him!", "question_pos_tagged": ["PRP", "MD", "VB", "PRP", ".", "PRP", "MD", "VB", "PRP", "."], "aSentId": 55, "answer": "No, Sam!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"PERSON": ["Sam!"]}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "NN", "NN", "."], "qSentId": 56, "question": "Alight, I'm sorry, I didn't mean for it to go so far, I was so angry . . . Here, let's rest up a bit.", "question_pos_tagged": ["NNP", ",", "PRP", "VBP", "JJ", ",", "PRP", "VBD", "RB", "VB", "IN", "PRP", "TO", "VB", "RB", "RB", ",", "PRP", "VBD", "RB", "JJ", ".", ".", ".", "RB", ",", "VB", "POS", "NN", "IN", "DT", "NN", "."], "aSentId": 56, "answer": "I ' m alright.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["NN", "RB", "."], "qSentId": 57, "question": "No, no, you're not alright! You're exhausted! It's that Gollum; it's this place; it's that thing around your neck. I could help a bit . . . I could carry it for a while - share the load.", "question_pos_tagged": ["DT", ",", "DT", ",", "PRP", "VBP", "RB", "VBN", ".", "PRP", "VBP", "VBN", ".", "PRP", "VBZ", "IN", "NNP", ":", "PRP", "VBZ", "DT", "NN", ":", "PRP", "VBZ", "IN", "NN", "IN", "PRP$", "NNP", "NNP", "MD", "VB", "DT", "NN", ".", ".", ".", "PRP", "MD", "VB", "PRP", "IN", "DT", "NN", ":", "NN", "DT", "NN", "."], "aSentId": 57, "answer": "Get away!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"LOCATION": ["Gollum"]}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["DT", ",", "NNP", ".", ".", ".", "PRP", "VBZ", "PRP", "."], "qSentId": 59, "question": "Shut up, you! Go away. Get out of here.", "question_pos_tagged": ["NNP", "IN", ",", "PRP", ".", "NNP", "NNP", "NNP", "IN", "IN", "RB", "."], "aSentId": 59, "answer": "No, Sam . . . it's you.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"PERSON": ["Sam"]}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["PRP", "MD", "RB", "VB", "PRP", "VB", "."], "qSentId": 60, "question": "But . . . he's a liar . . . he's poisoned you against me.", "question_pos_tagged": ["CC", ".", ".", ".", "PRP", "VBZ", "DT", "JJ", ".", ".", ".", "PRP", "VBZ", "VBN", "PRP", "IN", "PRP", "."], "aSentId": 60, "answer": "You can't help me anymore.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["NNP", "NN", "."], "qSentId": 61, "question": "You don't mean that.", "question_pos_tagged": ["PRP", "VBP", "RB", "VB", "IN", "."], "aSentId": 61, "answer": "Go home.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "EOWYN", "speaker2": "EOMER", "docId": 382, "answer_pos_tagged": ["PRP", "MD", "RB", "VB", "PRP", "."], "qSentId": 62, "question": "Well, that's no good. You won't kill many Ores with a blunt blade. Come on. To the Smithy .- go!", "question_pos_tagged": ["NNP", ",", "IN", "POS", "DT", "NNP", "NNP", "MD", "RB", "VB", "JJ", "NNS", "IN", "DT", "NN", "NNP", "NNP", "NNP", "NNP", "DT", "NNP", ":", "VB", "."], "aSentId": 62, "answer": "You should not encourage him.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"ORGANIZATION": ["Smithy"]}}, {"speaker1": "EOWYN", "speaker2": "EOMER", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "RB", "VB", "PRP$", "NN", ":", "RB", "DT", "NN", "IN", "PRP$", "NN", "."], "qSentId": 63, "question": "And you should not doubt him.", "question_pos_tagged": ["CC", "PRP", "MD", "RB", "VB", "PRP", "."], "aSentId": 63, "answer": "I do not doubt his heart ... Only the reach of his arm.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "ELROND", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "IN", "NN", "IN", "CD", "NN", "PRP", "VBP", "."], "qSentId": 65, "question": "My Lord, Elrond.", "question_pos_tagged": ["PRP$", "NNP", ",", "NNP", "."], "aSentId": 65, "answer": "I come on behalf of one whom I love.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "ELROND", "docId": 382, "answer_pos_tagged": ["PRP", "MD", "RB", "RB", "VB", "DT", "JJ", "IN", "RB", "VBZ", "IN", "NNP", "."], "qSentId": 66, "question": "She stayed?", "question_pos_tagged": ["PRP", "VBD", "."], "aSentId": 66, "answer": "She will not long survive the evil that now spreads from Mordor.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "ELROND", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "TO", "NN", ",", "CC", "RB", "TO", "NNP", "NNP", "POS", "NNS", "VBP", "IN", "NNP", "NNP", ":", "DT", "PRP", "VBP", ":", "CC", "IN", "NN", "PRP", "VBZ", "DT", "NN", "WDT", "MD", "VB", "IN", "DT", "NN", ".", ".", "."], "qSentId": 67, "question": "It will not be our end, but his.", "question_pos_tagged": ["PRP", "MD", "RB", "VB", "PRP$", "NN", ",", "CC", "PRP$", "."], "aSentId": 67, "answer": "You ride to war, but not to victory. Sauron's armies march on Minas Tirith - this you know - but in secret he sends another force which will attack from the river . . .", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"ORGANIZATION": ["Minas Tirith"]}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "ELROND", "docId": 382, "answer_pos_tagged": ["EX", "VBP", "DT", "WP", "RB", "IN", "DT", "NN", "."], "qSentId": 68, "question": "There are none.", "question_pos_tagged": ["EX", "VBP", "NN", "."], "aSentId": 68, "answer": "There are those who dwell in the mountain.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "ELROND", "docId": 382, "answer_pos_tagged": ["PRP", "MD", "VB", "TO", "DT", "NNP", "IN", "NNP", "."], "qSentId": 69, "question": "Murderers, traitors - you would call upon them to fight? They believe in nothing. They answer to no-one.", "question_pos_tagged": ["NNS", ",", "NNS", ":", "PRP", "MD", "VB", "IN", "PRP", "TO", "VB", ".", "PRP", "VBP", "IN", "NNP", "NNP", "NN", "TO", "JJ", "."], "aSentId": 69, "answer": "They will answer to the King of Condor.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"ORGANIZATION": ["King of Condor"]}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "ELROND", "docId": 382, "answer_pos_tagged": ["DT", "NN", "WP", "MD", "VB", "DT", "NN", "IN", "DT", "NN", "MD", "VB", "TO", "PRP", "DT", "NN", "JJR", "RB", "IN", "DT", "WDT", "VBZ", "DT", "NN", "."], "qSentId": 70, "question": "Sauron will not have forgotten the sword of Elendil . . .", "question_pos_tagged": ["NNP", "MD", "RB", "VB", "VBN", "DT", "NN", "IN", "NNP", ".", ".", "."], "aSentId": 70, "answer": "The man who can wield the power of this sword can summon to him an army more deadly than any that walks this earth.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "EOWYN", "docId": 382, "answer_pos_tagged": ["WRB", "VBP", "PRP", "VBG", "DT", "."], "qSentId": 72, "question": "U-chebin estel anim. I keep none for myself.", "question_pos_tagged": ["JJ", "NN", "NNP", "PRP", "VBP", "NN", "IN", "PRP", "."], "aSentId": 72, "answer": "Why are you doing this?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "ARAGORN", "speaker2": "EOWYN", "docId": 382, "answer_pos_tagged": ["NNP", "PRP", "RB", "VB", "."], "qSentId": 73, "question": "Eowyn . . . Why have you come?", "question_pos_tagged": ["NNP", ".", ".", ".", "WRB", "VBP", "PRP", "VBP", "."], "aSentId": 73, "answer": "Do you not know?", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "THEODEN", "speaker2": "MERRY", "docId": 382, "answer_pos_tagged": ["DT", "PRP$", "NNS", "VBP", "VBN", "TO", "NNP", "PRP", "MD", "VB", "VBN", "TO", "VB", "VBN", "IN", "."], "qSentId": 75, "question": "We must ride light and swift. It's a long road ahead ... and man and beast must reach the end with the strength to fight.", "question_pos_tagged": ["PRP", "MD", "VB", "JJ", "CC", "NNP", "NNP", "POS", "DT", "JJ", "NN", "RB", ":", "CC", "NN", "CC", "JJ", "MD", "VB", "DT", "NN", "IN", "DT", "NN", "TO", "NN", "."], "aSentId": 75, "answer": "All my friends have gone to battle. I would be ashamed to be left behind!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "THEODEN", "speaker2": "MERRY", "docId": 382, "answer_pos_tagged": ["CC", ".", ".", ".", "PRP", "VBP", "TO", "NN", "."], "qSentId": 76, "question": "It is a three day gallop to Minas Tirith. None of my Riders can bear you as a burden.", "question_pos_tagged": ["PRP", "VBZ", "DT", "CD", "NN", "NN", "TO", "NNP", "NNP", "NNP", "IN", "PRP$", "NNP", "MD", "VB", "PRP", "IN", "DT", "NN", "."], "aSentId": 76, "answer": "But . . . I want to fight!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"ORGANIZATION": ["Minas Tirith"]}}, {"speaker1": "KING OF THE DEAD", "speaker2": "ARAGORN", "docId": 382, "answer_pos_tagged": ["CD", "WP", "MD", "VB", "PRP$", "NN", "."], "qSentId": 78, "question": "Who enters my domain?", "question_pos_tagged": ["WP", "NNS", "PRP$", "NN", "."], "aSentId": 78, "answer": "One who will have your allegiance.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "KING OF THE DEAD", "speaker2": "ARAGORN", "docId": 382, "answer_pos_tagged": ["PRP", "MD", "VB", "PRP", ".", "PRP", "VBP", "PRP", "TO", "VB", "PRP$", "NN", "."], "qSentId": 79, "question": "The dead do not suffer the living to pass ...", "question_pos_tagged": ["DT", "NN", "VBP", "RB", "VB", "DT", "NN", "TO", "VB", ":"], "aSentId": 79, "answer": "You will suffer me! I summon you to fulfil your oath!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "KING OF THE DEAD", "speaker2": "ARAGORN", "docId": 382, "answer_pos_tagged": ["PRP", "VBZ", "VBN", "VBN", "."], "qSentId": 80, "question": "None but the King of Gondor may command me!", "question_pos_tagged": ["NN", "CC", "DT", "NNP", "IN", "NNP", "MD", "VB", "PRP", "."], "aSentId": 80, "answer": "It has been remade!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"ORGANIZATION": ["King of Gondor"]}}, {"speaker1": "DENETHOR", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["PRP", "VBZ", "JJ", "."], "qSentId": 81, "question": "My son is dead. My line is ended.", "question_pos_tagged": ["PRP$", "NN", "VBZ", "NNP", "NNP", "NN", "VBZ", "VBN", "."], "aSentId": 81, "answer": "He's alive!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "DENETHOR", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": [":", "PRP", "VBZ", "NN", ",", "PRP$", "NNP", "."], "qSentId": 82, "question": "The House of Stewards has failed! . . .", "question_pos_tagged": ["DT", "NNP", "IN", "NNPS", "VBZ", "VBN", ".", ".", ".", "."], "aSentId": 82, "answer": "; He needs medicine, my Lord!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"ORGANIZATION": ["House of Stewards"]}}, {"speaker1": "DENETHOR", "speaker2": "PIPPIN", "docId": 382, "answer_pos_tagged": ["PRP$", "NNP", "."], "qSentId": 83, "question": "My line has ended.", "question_pos_tagged": ["PRP$", "NN", "VBZ", "VBN", "."], "aSentId": 83, "answer": "My Lord!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "GORBAG", "docId": 382, "answer_pos_tagged": ["WP", "VBZ", "DT", ".", "NNP", "IN", "NNP", "NNP", "POS", "VBN", "VBG", "DT", "NN", "IN", "NN", ".", "DT", ".", ".", ".", "DT", "JJ", "NN", "RB", "VB", "."], "qSentId": 85, "question": "Let him go, you filth!", "question_pos_tagged": ["NNP", "PRP", "VBP", ",", "PRP", "VBP", "."], "aSentId": 85, "answer": "What's this? Looks like Old Shelob's been having a bit of fun! No . . . this fellow ain't dead.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"ORGANIZATION": ["Old Shelob"]}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "GORBAG", "docId": 382, "answer_pos_tagged": [".", "PRP", "VBZ", "PRP", "IN", "PRP$", "NN", "CC", "PRP", "VBP", "RB", "VB", "IN", "DT", "VBN", "NNP", "NNP", "PRP", "VBZ", "PRP$", "NN", "IN", "NNP", "NNP", "POS", "WRB", "PRP", "VBZ", "TO", "VB", ":", "JJ", "NNP", "NNP", "PRP", "TO", "DT", "NNP", "."], "qSentId": 86, "question": "Not dead!", "question_pos_tagged": ["RB", "VBN", "."], "aSentId": 86, "answer": ". She jabs them with her stinger and they go as limp as a boned fish. Then she has her way with them. That's how she likes to feed - fresh blood. Get him to the Tower!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "GORBAG", "docId": 382, "answer_pos_tagged": ["RB", "PRP", "MD", "VB", "PRP", "MD", "RB", "VBN", "VBN", "."], "qSentId": 87, "question": "Samwise, you fool!", "question_pos_tagged": ["NNP", ",", "PRP", "VBP", "."], "aSentId": 87, "answer": "Then he'll wish he'd never been born.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "PIPPIN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["NN", ".", "NNP", ",", "DT", "NN", "VBZ", "RB", "VB", "RB", "."], "qSentId": 88, "question": "I didn't think it would end this way . . .", "question_pos_tagged": ["PRP", "VBD", "RB", "VB", "PRP", "MD", "VB", "DT", "NN", ".", ".", "."], "aSentId": 88, "answer": "End? No, the journey doesn't end here.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "PIPPIN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["NNP", "NNS", ":", "CC", "IN", ".", ".", ".", "DT", "NN", "VBN", "NN", "IN", "DT", "NN", "NN", "."], "qSentId": 89, "question": "What, Gandalf? See what?", "question_pos_tagged": ["WP", ",", "NNP", ".", "NNP", "WP", "."], "aSentId": 89, "answer": "White shores ... And beyond . . . A far green country under a swift sunrise.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"ORGANIZATION": ["Gandalf"]}}, {"speaker1": "PIPPIN", "speaker2": "GANDALF", "docId": 382, "answer_pos_tagged": ["DT", ".", ".", ".", "DT", ",", "PRP", "VBZ", "RB", "."], "qSentId": 90, "question": "Well, that isn't so bad.", "question_pos_tagged": ["NNP", ",", "IN", "VBZ", "RB", "RB", "JJ", "."], "aSentId": 90, "answer": "No . . . No, it isn't.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["NNP", ":", ".", "."], "qSentId": 91, "question": "Not if I stick you first!", "question_pos_tagged": ["RB", "IN", "PRP", "VBP", "PRP", "RB", "."], "aSentId": 91, "answer": "Sam!!!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["PRP", "VBZ", "RB", "NNP", "PRP", "VBZ", "IN", ".", ".", ".", "PRP", "VBP", "VBN", "NNP", "NNP", ":", "PRP", "VBD", "DT", "NNP", ".", "."], "qSentId": 92, "question": "Come on.", "question_pos_tagged": ["NNP", "IN", "."], "aSentId": 92, "answer": "It's too late. It's over . . . they've taken it. Sam - they took the Ring!!", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["JJ", "PRP", "TO", "PRP", "."], "qSentId": 93, "question": "Begging your pardon, but they haven't.", "question_pos_tagged": ["VBG", "PRP$", "NN", ",", "CC", "PRP", "VBP", "RB", "."], "aSentId": 93, "answer": "Give it to me.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["EX", "VBP", "RB", "JJ", "IN", "PRP", ".", ".", ".", "RB", "NNP", "NNP", "NNP", "RB", "VB", "IN", "VBN", "."], "qSentId": 94, "question": "Come on, Mr Frodo, we'd best find you some clothes . . . you can't go walking through Mordor in naught but your skin. We did it, Mr Frodo. We made it to Mordor.", "question_pos_tagged": ["NNP", "IN", ",", "NNP", "NNP", ",", "PRP", "MD", "JJS", "VB", "PRP", "DT", "NNS", ".", ".", ".", "PRP", "MD", "RB", "VB", "VBG", "IN", "NNP", "IN", "NN", "CC", "PRP$", "NNP", "NNP", "VBD", "PRP", ",", "NNP", "NNP", "NNP", "VBD", "PRP", "TO", "NNP", "."], "aSentId": 94, "answer": "There are so many of them . . . So many. We'll never get through unseen.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"PERSON": ["Mr Frodo", "Mr Frodo"]}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["PRP", "VBP", "RB", ",", "NNP", "."], "qSentId": 96, "question": "Frodo!", "question_pos_tagged": ["NNP", ":"], "aSentId": 96, "answer": "I'm here, Sam.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {"PERSON": ["Sam"]}, "question_ne_tagged": {}}, {"speaker1": "SAM", "speaker2": "FRODO", "docId": 382, "answer_pos_tagged": ["DT", "NNP", "VBZ", "JJ", "."], "qSentId": 97, "question": "Destroy it - go on! Throw it in the fire!", "question_pos_tagged": ["NNP", "PRP", ":", "VB", "IN", ".", "NNP", "PRP", "IN", "DT", "NN", "."], "aSentId": 97, "answer": "The Ring is mine.", "movie_name": "Lord-of-the-Rings-Return-of-the-King.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}]