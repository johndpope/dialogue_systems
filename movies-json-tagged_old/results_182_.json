[{"speaker1": "DANIEL", "speaker2": "HOT ROD", "docId": 182, "answer_pos_tagged": ["DT", "``", "NN", "''", "NNP", "NNP", "VBZ", "IN", "NN", "NN", ",", "VBG", "IN", "PRP$", "NN", "NN", "VBD", "IN", "CD", "NNS", ",", "VBG", "VBN", "IN", "PRP", "NNP", "NNP", "IN", "JJ", ":", ":", "."], "qSentId": 0, "question": "Let's watch Ironhide land it, Hot Rod! As Daniel runs to small skateboard-like device, hits a button, and watches, it transforms into a hoverboard and rides it, like a surfer up a steep, grassy hillside we change angle to feature...", "question_pos_tagged": ["NNP", "POS", "NN", "NNP", "NN", "PRP", ",", "NNP", "NNP", ".", "NNP", "NNP", "VBZ", "TO", "JJ", "JJ", "NN", ",", "VBZ", "DT", "NN", ",", "CC", "NNS", ",", "PRP", "VBZ", "IN", "DT", "JJ", "CC", "VBZ", "PRP", ",", "IN", "DT", "NN", "RP", "DT", "NN", ",", "JJ", "NN", "PRP", "VBP", "JJ", "TO", "VB", ":"], "aSentId": 0, "answer": "an \"adolescent\" Autobot. He is in robot mode, sitting with his fishing rod held between two fingers, looking bored with it all. Talk about dull......!", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"PERSON": ["Daniel"]}}, {"speaker1": "DANIEL", "speaker2": "HOT ROD", "docId": 182, "answer_pos_tagged": ["NNS", "IN", "NN", "IN", "NNP", "NNP", "PRP", "VBP", "JJ", "NN", "NN", ",", "NNP", ":", "NN", "IN", "NN", ".", "NNP", "NNP", "VBZ", "RP", "IN", "NNP", ",", "NNS", "PRP", "RP", "PRP$", "NN", ",", "NNP", "TO", "VB", "NN", "CC", "NNP", "NNP", "NNP", "NNP", "NNP", "IN", "NNP", "IN", "DT", "NN", "NN", "."], "qSentId": 1, "question": "Hurry or we'll miss it!", "question_pos_tagged": ["NNP", "CC", "PRP", "MD", "VB", "PRP", "."], "aSentId": 1, "answer": "lopes up hill after Daniel. If you're gonna ride, Dan-O ...ride in style! Hot Rod catches up with Daniel, scoops him off his hoverboard, TRANSFORMS to automotive mode and RIDES OVER CREST OF HILL with Daniel in the front seat.", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {"PERSON": ["Daniel", "Daniel", "Daniel"]}, "question_ne_tagged": {}}, {"speaker1": "DANIEL", "speaker2": "HOT ROD", "docId": 182, "answer_pos_tagged": ["WRB", "JJ", "IN", "DT", "NN", "WRB", "PRP", "MD", "VB", "NN", "IN", "NNP", "NNP", "."], "qSentId": 2, "question": "Hey! Let me out.", "question_pos_tagged": ["PRP", "VBP", "NNP", "PRP", "RP", "."], "aSentId": 2, "answer": "Why settle for a peek when you can see everything from Lookout Mountain?", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "DANIEL", "speaker2": "HOT ROD", "docId": 182, "answer_pos_tagged": ["JJ", "NN"], "qSentId": 3, "question": "But we're not supposed to go beyond City limits!", "question_pos_tagged": ["CC", "PRP", "VBP", "RB", "VBN", "TO", "VB", "IN", "NNP", "NNS", "."], "aSentId": 3, "answer": "Chromo-Chicken!", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"LOCATION": ["City"]}}, {"speaker1": "BLASTER", "speaker2": "PERCEPTOR", "docId": 182, "answer_pos_tagged": ["NNP", ",", "NNP", ".", "NNP", "PRP", "."], "qSentId": 4, "question": "We got Cassette Critters knockin' on the window!", "question_pos_tagged": ["PRP", "VBD", "NNP", "NNP", "NN", "POS", "IN", "DT", "NN", "."], "aSentId": 4, "answer": "Run, Blaster! Save yourself!", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"ORGANIZATION": ["Cassette Critters"]}}, {"speaker1": "BLASTER", "speaker2": "PERCEPTOR", "docId": 182, "answer_pos_tagged": ["PRP", "VBP", "PRP", "VBD", "IN", "TO", "NNP", "."], "qSentId": 5, "question": "No way. Two can play! Sic 'em!", "question_pos_tagged": ["DT", "NNP", "NNP", "MD", "VB", ".", "NNP", "NNP", "."], "aSentId": 5, "answer": "You think you got through to Prime?", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "MEGATRON", "speaker2": "PRIME", "docId": 182, "answer_pos_tagged": ["DT", "NN", "``", "NN", "''", "NNS", "PRP$", "NN", "CC", "NNS", "IN", "PRP", "IN", ":", ":", "PRP", "VBZ", "NNP", ",", ":", "VBG", "NNS", "IN", "NNP", "POS", "NN", "TO", ":", ":", "NN", "DT", "JJ", "NN", "WDT", ":", ":", "NNS", "NNP", "IN", "DT", "NN", "RB", "JJ", "IN", ":", ":", "PRP$", "NN", "NNS", "RB", "IN", "VBG", "JJ", "NN", ",", "VBG", "PRP", "IN", ":"], "qSentId": 7, "question": "grabs a jagged, spearlike piece of steel and hurls it at", "question_pos_tagged": ["NNS", "DT", "JJ", ",", "IN", "NN", "IN", "NN", "CC", "NNS", "PRP", "IN"], "aSentId": 7, "answer": "The steel \"spear\" pierces his side and sticks in him as... ... he charges Megatron, ...dodging shots from Megatron's gun to... ...deliver a huge UPPERCUT which... ...lifts Megatron into the air so high that... ... his head butts sharply against overhanging concrete balcony, mashing him down...", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {"ORGANIZATION": ["Megatron"]}, "question_ne_tagged": {}}, {"speaker1": "MEGATRON", "speaker2": "PRIME", "docId": 182, "answer_pos_tagged": ["IN", "PRP", "VBZ", "IN", "PRP", ",", "VBG", ":", ":", "DT", "NN", "IN", "NNP", "POS", "NN", ",", "WDT", "NNS", "VBP", "CC", "NNS", "CC", ":"], "qSentId": 8, "question": "lands and stealthily slips a laser dagger from his boot, turning and slashing at...", "question_pos_tagged": ["NNS", "CC", "RB", "VBZ", "DT", "NN", "NN", "IN", "PRP$", "NN", ",", "VBG", "CC", "VBG", "IN", ":"], "aSentId": 8, "answer": "as he comes for him, tearing... ...a gash across Prime's midsection, which spews smoke and sparks and...", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["WP", "VBD", "IN", "."], "qSentId": 10, "question": "Welcome, Megatron.", "question_pos_tagged": ["NNP", ",", "NNP", "."], "aSentId": 10, "answer": "Who said that?", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["NNP", "PRP", "."], "qSentId": 11, "question": "I am Unicron.", "question_pos_tagged": ["PRP", "VBP", "NNP", "."], "aSentId": 11, "answer": "Show yourself!", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["NNP", "NNS", "NNP", "."], "qSentId": 12, "question": "I have summoned you here for a purpose.", "question_pos_tagged": ["PRP", "VBP", "VBN", "PRP", "RB", "IN", "DT", "NN", "."], "aSentId": 12, "answer": "Nobody summons Megatron!", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["NNP", "PRP$", "NN", "."], "qSentId": 13, "question": "Then it pleases me to be the first.", "question_pos_tagged": ["RB", "PRP", "VBZ", "PRP", "TO", "VB", "DT", "JJ", "."], "aSentId": 13, "answer": "State your business.", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["PRP", "VBP", "NN", "TO", "NNP", "PRP", "VBP", "RB", "VBN", "NNP", "NNP", "IN", "PRP$", "NN", "NNS", "."], "qSentId": 14, "question": "Bring me the Autobot Matrix. It is the one thing - the only thing that can stand in my way.", "question_pos_tagged": ["VBG", "PRP", "DT", "NNP", "NNP", "NNP", "VBZ", "DT", "CD", "NN", ":", "DT", "JJ", "NN", "WDT", "MD", "VB", "IN", "PRP$", "NN", "."], "aSentId": 14, "answer": "You have nothing to fear. I have already crushed Optimus Prime with my bare hands.", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {"ORGANIZATION": ["Optimus"]}, "question_ne_tagged": {"ORGANIZATION": ["Autobot Matrix"]}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["DT", "NN", "VBZ", ",", "PRP", "VBZ", "JJ", ".", "CC", "DT", "NNP", "VBD", "IN", "PRP", "."], "qSentId": 15, "question": "You exaggerate.", "question_pos_tagged": ["PRP", "VBP", "."], "aSentId": 15, "answer": "The point is, he is dead! And the Matrix died with him.", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["WRB", "MD", "PRP", ".", "WP", "VBZ", "IN", "PRP", "IN", "PRP", "."], "qSentId": 16, "question": "No. It has been passed to their new leader. Get it for me.", "question_pos_tagged": ["NN", "PRP", "VBZ", "VBN", "VBN", "TO", "PRP$", "JJ", "NNP", "NNP", "PRP", "IN", "PRP", "."], "aSentId": 16, "answer": "Why should I? What's in it for me?", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["CC", ":"], "qSentId": 17, "question": "Your bargaining posture is highly dubious, but very well, I will provide you with a new body, and new troops to command.", "question_pos_tagged": ["PRP$", "NN", "NN", "VBZ", "RB", "JJ", ",", "CC", "RB", "RB", ",", "PRP", "MD", "VB", "PRP", "IN", "DT", "JJ", "NN", ",", "CC", "JJ", "NNS", "TO", "VB", "."], "aSentId": 17, "answer": "And...", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["PRP", "VBP", "TO", "NN", "."], "qSentId": 18, "question": "And nothing.", "question_pos_tagged": ["CC", "NN", "."], "aSentId": 18, "answer": "I belong to nobody.", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "MEGATRON", "docId": 182, "answer_pos_tagged": ["DT", ",", "NNP", "PRP", "VBD", "PRP$", "NNP", "NNP", "VBD", "."], "qSentId": 19, "question": "Perhaps I misjudged you. Proceed on your way to oblivion. The Mist appears and starts to dissolve Megatron.", "question_pos_tagged": ["RB", "PRP", "VBD", "NNP", "NNP", "IN", "PRP$", "NN", "TO", "NNP", "DT", "NNP", "VBZ", "CC", "VBZ", "TO", "VB", "NNP", "."], "aSentId": 19, "answer": "No, no. I accept your terms. I accept!", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "GALVATRON", "docId": 182, "answer_pos_tagged": ["JJ", "JJ", "NNP", "."], "qSentId": 21, "question": "Excellent! Behold - Galvatron! X-ray images", "question_pos_tagged": ["NNP", ":", "NNP", ":", "NNP", ".", "NNP", "NNS"], "aSentId": 21, "answer": "Long live Galvatron!", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "GALVATRON", "docId": 182, "answer_pos_tagged": ["TO", "NNP", "."], "qSentId": 22, "question": "And these shall be your minions... Scourge, the tracker....and his huntsmen...the Sweeps.... Cyclonus, the warrior...and his armada.....", "question_pos_tagged": ["CC", "DT", "MD", "VB", "PRP$", "NNS", ":", "NNP", ",", "DT", "NN", ":", "JJ", "PRP$", "NNS", ":", "DT", "NNP", ":", ".", "NNP", ",", "DT", "NN", ":", "CC", "PRP$", "NN", ":", ":"], "aSentId": 22, "answer": "To Cybertron!", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "UNICRON", "speaker2": "GALVATRON", "docId": 182, "answer_pos_tagged": ["PRP", "MD", "VB", "JJ", "NNP", "IN", "NNP", "IN", "PRP", "VBZ", "NNS", ",", "NNP", "."], "qSentId": 23, "question": "Bring me the Autobot matrix! DURING UNICRON'S COMMAND we CHANGE ANGLE TO SHOW", "question_pos_tagged": ["VBG", "PRP", "DT", "NNP", "NN", ".", "NNP", "NNP", "POS", "NNP", "PRP", "NNP", "NNP", "NNP", "NNP"], "aSentId": 23, "answer": "I will rip open Autobot after Autobot until it is yours, Unicron.", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "GALVATRON", "speaker2": "STARSCREAM", "docId": 182, "answer_pos_tagged": ["NN", "IN", "IN", "DT", "NN", ".", "NNP", "CC", "NNS", "VBP", "TO", "NNP", "NNP", "CC", "VB", "RP", "CC", "NNP", "NNP", "VBZ", "WRB", "NNS", "VBP", "VBG", "CC", "NNP", "NNP", "VBZ", "PRP$", "NN", "."], "qSentId": 24, "question": "I will rip open Autobot after Autobot until it is yours, Unicron.", "question_pos_tagged": ["PRP", "MD", "VB", "JJ", "NNP", "IN", "NNP", "IN", "PRP", "VBZ", "NNS", ",", "NNP", "."], "aSentId": 24, "answer": "Get on with the ceremony! Dirge and others react to EERIE SOUNDS and look up and off. Starscream looks where others are looking and reacts. Who disrupts my coronation?", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "GALVATRON", "speaker2": "STARSCREAM", "docId": 182, "answer_pos_tagged": ["NNP", ",", "VBZ", "IN", "PRP", "."], "qSentId": 25, "question": "Coronation? This is bad comedy!", "question_pos_tagged": ["NN", ".", "DT", "VBZ", "JJ", "NN", "."], "aSentId": 25, "answer": "Megatron, is that you?", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {}}, {"speaker1": "SPIKE", "speaker2": "BUMBLEBEE", "docId": 182, "answer_pos_tagged": ["DT", "NN", ":", "DT", "NN", "RB", "VBD", "DT", "JJ", "NN", "TO", "NNS", "."], "qSentId": 27, "question": "It's like a terrible nightmare, Ultra Magnus.", "question_pos_tagged": ["PRP", "VBZ", "IN", "DT", "JJ", "NN", ",", "NNP", "NNP", "."], "aSentId": 27, "answer": "This thing - this juggernaut just ripped the first moon to shreds.", "movie_name": "Transformers-The-Movie.dial", "answer_ne_tagged": {}, "question_ne_tagged": {"ORGANIZATION": ["Ultra Magnus"]}}, {"speaker1": "SPIKE", "speaker2": "BUMBLEBEE", "docId": 182, "answer_pos_tagged": ["PRP", "MD", "VB", "JJ", "JJ", "PRP", "IN", ":"], "qSentId": 28, "question": "And it's heading this way.", "question_pos_tagged": ["CC", "PRP", "VBZ", "VBG", "DT", "NN", "."], "aS