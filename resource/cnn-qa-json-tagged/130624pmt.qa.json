[{"answer_ne_tagged": {}, "docId": "130624pmt", "answer_pos_tagged": ["PRP", "VBP", "JJ", "."], "qSentId": 15, "question": "Rudy, how are you?", "question_pos_tagged": ["NNP", ",", "WRB", "VBP", "PRP", "."], "aSentId": 16, "answer": "I'm good.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "130624pmt", "answer_pos_tagged": ["NNP", ",", "PRP", "VBP", "JJ", ",", "CC", "PRP", "VBP", "RB", "VB", "WP", "TO", "VB", "IN", "DT", "IN", "DT", "."], "qSentId": 17, "question": "How are you, Piers?", "question_pos_tagged": ["WRB", "VBP", "PRP", ",", "NNP", "."], "aSentId": 18, "answer": "Well, I'm good, but I don't know what to make of all of this.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "130624pmt", "answer_pos_tagged": ["RB", "."], "qSentId": 31, "question": "Right?", "question_pos_tagged": ["RB", "."], "aSentId": 32, "answer": "Right.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "130624pmt", "answer_pos_tagged": ["EX", "VBZ", ":", "EX", "RB", "VBZ", "RB", "DT", "JJ", "NN", ",", "EX", "VBZ", "DT", "JJ", "NN", "."], "qSentId": 42, "question": "Do you think there is a reasonable defense for putting into the public domain the material that he reasonably judiciously chose to put out there?", "question_pos_tagged": ["NNP", "PRP", "VBP", "RB", "VBZ", "DT", "JJ", "NN", "IN", "VBG", "IN", "DT", "NN", "NN", "DT", "NN", "IN", "PRP", "RB", "RB", "VB", "TO", "VB", "RP", "RB", "."], "aSentId": 43, "answer": "There's -- there really isn't a legal defense, there's an emotional defense.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "130624pmt", "answer_pos_tagged": ["PRP", "MD", "VB", "PDT", "DT", "."], "qSentId": 46, "question": "Is there a public interest defense?", "question_pos_tagged": ["VBZ", "EX", "DT", "JJ", "NN", "NN", "."], "aSentId": 47, "answer": "You can try all that.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "130624pmt", "answer_pos_tagged": ["NNP", ",", "PRP", "VBP", "DT", "NN", "VBZ"