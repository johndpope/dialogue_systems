[{"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", "PRP", "VBZ", "RB", "JJ", "VBG", "RP", "IN", "NN", "."], "qSentId": 12, "question": "Condoleezza Rice, is it more fun being in power or is it more fun after power?", "question_pos_tagged": ["NNP", "NNP", ",", "VBZ", "PRP", "JJR", "IN", "VBG", "IN", "NN", "CC", "VBZ", "PRP", "JJR", "IN", "IN", "NN", "."], "aSentId": 13, "answer": "Well it's certainly different being out of power.", "question_ne_tagged": {"PERSON": ["Condoleezza Rice"]}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "PRP", "VBZ", "JJR", "IN", "WP", "PRP", "MD", "VB", "RB", "IN", "PRP", "MD", "RB", "VB", "RB", "IN", ":", "EX", "VBZ", "RB", "DT", "NN", "IN", "NN", "IN", "DT", "NNS", "."], "qSentId": 25, "question": "But in practical reality what can you now do you couldn't do, that you missed?", "question_pos_tagged": ["CC", "IN", "JJ", "NN", "WP", "MD", "PRP", "RB", "VBP", "PRP", "MD", "RB", "VB", ",", "IN", "PRP", "VBD", "."], "aSentId": 26, "answer": "Well, it's less of what I can do now that I couldn't do then than -- there isn't the sort of relentlessness of the days.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", "PRP", "VBD", "VBG", "TO", "VB", "DT", "NN", "NN", "."], "qSentId": 55, "question": "Did you ever imagine in your wildest dreams that one day you'd be in there?", "question_pos_tagged": ["NNP", "PRP", "RB", "VBP", "IN", "PRP$", "NN", "NNS", "WDT", "CD", "NN", "PRP", "MD", "VB", "IN", "EX", "."], "aSentId": 56, "answer": "Well I was going to be a concert pianist.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", "VBP", "DT", "NN", "NN", ":"], "qSentId": 79, "question": "Because you're a mad football nut, aren't you?", "question_pos_tagged": ["IN", "PRP", "VBP", "DT", "NN", "NN", "NN", ",", "VBP", "RB", "PRP", "."], "aSentId": 80, "answer": "I'm a mad football --", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "IN", "VBD", "WP", "VBD", "JJ", "."], "qSentId": 89, "question": "What values did they give you?", "question_pos_tagged": ["WP", "NNS", "VBD", "PRP", "VBP", "PRP", "."], "aSentId": 90, "answer": "Well, that was what was extraordinary.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NN", ":", "NNP", ",", "IN", "EX", "VBZ", "DT", "JJR", "NN", "CC", "EX", "VBZ", "DT", "JJR", "NN", "IN", "TO", "VB", "IN", "JJ", "NN", "."], "qSentId": 128, "question": "And why would you convince yourself that you had done this terrible, God-forsaken job?", "question_pos_tagged": ["CC", "WRB", "MD", "PRP", "VB", "PRP", "IN", "PRP", "VBD", "VBN", "DT", "JJ", ",", "JJ", "NN", "."], "aSentId": 129, "answer": "ICE: Well, because there's no greater challenge and there is no greater honor than to be in public service.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", "DT", ",", "PRP", "VBP", ":", "PRP", "VBP", "RB", "RB", "IN", "DT", "."], "qSentId": 142, "question": "You're not far off, are you?", "question_pos_tagged": ["PRP", "VBP", "RB", "RB", "IN", ",", "VBP", "PRP", "."], "aSentId": 143, "answer": "Well no, I'm -- I'm pretty far from that.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["RB", "RB", "."], "qSentId": 163, "question": "Anything you want to share with me?", "question_pos_tagged": ["NN", "PRP", "VBP", "TO", "VB", "IN", "PRP", "."], "aSentId": 164, "answer": "Not really.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "RB", "DT", "NN", "."], "qSentId": 187, "question": "Nothing but a sniffle?", "question_pos_tagged": ["NN", "CC", "DT", "NN", "."], "aSentId": 188, "answer": "Well, not a sniffle.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["CC", "PRP", ":"], "qSentId": 190, "question": "How?", "question_pos_tagged": ["WRB", "."], "aSentId": 191, "answer": "And I --", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "PRP", ":"], "qSentId": 192, "question": "How -- how do you stop getting a cold?", "question_pos_tagged": ["WRB", ":", "WRB", "VBP", "PRP", "VB", "VBG", "DT", "JJ", "."], "aSentId": 193, "answer": "Well, you --", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NN", ":", "PRP", "VBP", "RB", "VB", "NNS", "."], "qSentId": 199, "question": "What are they?", "question_pos_tagged": ["WP", "VBP", "PRP", "."], "aSentId": 200, "answer": "ICE: You don't get colds.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "RB", "."], "qSentId": 210, "question": "Keeping fit -- did you drink alcohol when you were in office?", "question_pos_tagged": ["VBG", "NN", ":", "VBD", "PRP", "VBP", "NN", "WRB", "PRP", "VBD", "IN", "NN", "."], "aSentId": 211, "answer": "Well, occasionally.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["RB", ",", "RB", "."], "qSentId": 212, "question": "Did you drink alcohol?", "question_pos_tagged": ["NNP", "PRP", "VBP", "NN", "."], "aSentId": 213, "answer": "Sure, occasionally.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["IN", "NN", "RB", "."], "qSentId": 217, "question": "-- can you ever actually get intoxicated?", "question_pos_tagged": [":", "MD", "PRP", "RB", "RB", "VB", "VBN", "."], "aSentId": 218, "answer": "Of course not.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", "VBP", "RB", "JJ", "."], "qSentId": 237, "question": "Are you still absurdly fit?", "question_pos_tagged": ["NNP", "PRP", "RB", "RB", "JJ", "."], "aSentId": 238, "answer": "I'm still fit.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["DT", "NN", "."], "qSentId": 245, "question": "Every day?", "question_pos_tagged": ["DT", "NN", "."], "aSentId": 246, "answer": "Every day.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["IN", "PRP", "VBP", "TO", "VB", "IN", "DT", "NN", "CC", "PRP", "VBP", "TO", "VB", "DT", "JJ", "NN", "IN", "PRP", "IN", "PRP", "VBP", "."], "qSentId": 253, "question": "Why do you do this?", "question_pos_tagged": ["WRB", "VBP", "PRP", "VB", "DT", "."], "aSentId": 254, "answer": "Because I like to exercise in the morning and I like to have the whole day before me after I exercise.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["IN", "DT", "NN", "."], "qSentId": 255, "question": "How -- how long do you do it for?", "question_pos_tagged": ["WRB", ":", "WRB", "JJ", "VBP", "PRP", "VB", "PRP", "IN", "."], "aSentId": 256, "answer": "About an hour.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", "VBP", "RB", "."], "qSentId": 259, "question": "Pump weights?", "question_pos_tagged": ["NNP", "NNS", "."], "aSentId": 260, "answer": "I do cardio.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", "VBP", "DT", "NN", "VBG", "."], "qSentId": 261, "question": "Kick boxing?", "question_pos_tagged": ["NNP", "NN", "."], "aSentId": 262, "answer": "I do some kick boxing.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", "VBP", "."], "qSentId": 263, "question": "Do you?", "question_pos_tagged": ["NNP", "PRP", "."], "aSentId": 264, "answer": "I do.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", "VBP", "RB", "VB", "."], "qSentId": 265, "question": "Who do you think of when you're smashing the -- the bag?", "question_pos_tagged": ["WP", "VBP", "PRP", "VB", "IN", "WRB", "PRP", "VBP", "VBG", "DT", ":", "DT", "NN", "."], "aSentId": 266, "answer": "I don't think.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", "."], "qSentId": 273, "question": "Do you really?", "question_pos_tagged": ["NNP", "PRP", "RB", "."], "aSentId": 274, "answer": "Hmm.", "question_ne_tagged": {}}, {"answer_ne_tagged": {"LOCATION": ["Birmingham"]}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "PRP", "VBP", "TO", ":", "JJ", "IN", "DT", ",", "NNP", "VBD", "RB", "DT", "JJ", "NN", "NN", "IN", "IN", "PRP", "VBD", "JJ", ",", "CC", "PRP", "VBD", "JJ", "TO", "VB", "IN", "DT", "RB", "VBG", "NN", ",", "RB", "VBG", "NN", "."], "qSentId": 290, "question": "It must be a great sadness, isn't it?", "question_pos_tagged": ["PRP", "MD", "VB", "DT", "JJ", "NN", ",", "VBZ", "RB", "PRP", "."], "aSentId": 291, "answer": "Well, I have to -- first of all, Birmingham was not an easy place grow up if you were black, but I was fortunate to live in a very loving family, very loving community.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "RB", ",", "PRP", "VBP", "PRP", "VBD", ":", "PRP", "VBP", ",", "RB", "NN", "IN", "PRP$", "NN", "POS", "NN", "."], "qSentId": 330, "question": "Did he make you tough?", "question_pos_tagged": ["NNP", "PRP", "VB", "PRP", "IN", "."], "aSentId": 331, "answer": "Well, certainly, I think I had -- I have, perhaps part of my father's personality.", "question_ne_tagged": {}}, {"answer_ne_tagged": {"DATE": ["2000"]}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "PRP", "VBD", "DT", "RB", "NN", "IN", "PRP", "VBD", "IN", "CD", "."], "qSentId": 335, "question": "What would he have made of what happened to you?", "question_pos_tagged": ["WP", "MD", "PRP", "VB", "VBN", "IN", "WP", "VBN", "TO", "PRP", "."], "aSentId": 336, "answer": "Well, he knew a little bit because he died in 2000.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", "PRP", "VBP", "PRP", "MD", "VB", "VBN", "VBN", "."], "qSentId": 345, "question": "She'd been amazed, do you think?", "question_pos_tagged": ["PRP", "MD", "VB", "VBN", ",", "VBP", "PRP", "VB", "."], "aSentId": 346, "answer": "Well I think she would have been amazed.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP$", "NN", "VBD", "DT", "NN", "IN", "DT", "NN", "NN", "IN", "DT", "NN", "."], "qSentId": 352, "question": "What kind of woman was she?", "question_pos_tagged": ["WP", "NN", "IN", "NN", "VBD", "PRP", "."], "aSentId": 353, "answer": "My mom was a lady in the nicest sense of the term.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "DT", "NN", "VBZ", "IN", "PRP$", "NNS", "VBZ", "RB", "JJ", "."], "qSentId": 403, "question": "To use the language of the gun in --in that kind of way?", "question_pos_tagged": ["TO", "VB", "DT", "NN", "IN", "DT", "NN", "IN", ":", "IN", "DT", "NN", "IN", "NN", "."], "aSentId": 404, "answer": "Well, the fact is that our politics is pretty rough.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": [":", "VBD", "PRP", "PRP", "."], "qSentId": 407, "question": "But should it -- should it be that rough?", "question_pos_tagged": ["CC", "MD", "PRP", ":", "MD", "PRP", "VB", "IN", "JJ", "."], "aSentId": 408, "answer": "-- experienced it myself.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "VB", "PRP", "RB", "VBP", ":", "DT", "IN", "DT", "VBD", ",", "PRP", "MD", "VB", "TO", "VB", "DT", "NNS", "NN", "IN", "."], "qSentId": 417, "question": "What about towards -- what about towards opponents?", "question_pos_tagged": ["WP", "IN", "NNS", ":", "WP", "IN", "NNS", "NNS", "."], "aSentId": 418, "answer": "Well, let me just say -- all of that said, I would like to see the politics cool down.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "PRP", "VBP", "RB", "JJ", "IN", "DT", "NN", "RB", "."], "qSentId": 429, "question": "How's President Obama doing, do you think?", "question_pos_tagged": ["WRB", "VBZ", "NNP", "NNP", "VBG", ",", "VBP", "PRP", "VB", "."], "aSentId": 430, "answer": "Well, I'm very fond of the president personally.", "question_ne_tagged": {"PERSON": ["Obama"]}}, {"answer_ne_tagged": {"LOCATION": ["America"]}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "PRP", "VBP", "IN", "PRP", "VBZ", "DT", "NN", "NN", "CC", "PRP", "VBZ", "VBG", "PRP$", "JJS", "IN", "DT", "NN", "CC", "PRP", "VBD", "RB", "RB", "VBN", "IN", "NNP", "VBD", "DT", "JJ", "NN", "."], "qSentId": 440, "question": "But could you have been tempted, do you think, in other circumstances, to vote for President Obama?", "question_pos_tagged": ["CC", "MD", "PRP", "VB", "VBN", "VBN", ",", "VBP", "PRP", "VB", ",", "IN", "JJ", "NNS", ",", "TO", "VB", "IN", "NNP", "NNP", "."], "aSentId": 441, "answer": "Well, I think that he is a fine person and he's doing his best for this country and I was personally quite gratified that America elected a black president.", "question_ne_tagged": {"PERSON": ["Obama"]}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", "VBZ", "RB", "VB", "PRP", "IN", "PRP$", "NN", "."], "qSentId": 452, "question": "Would you do it?", "question_pos_tagged": ["MD", "PRP", "VB", "PRP", "."], "aSentId": 453, "answer": "He doesn't need me in his administration.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", "VBP", "RB", "VB", "JJ", ",", "NNP", "."], "qSentId": 456, "question": "Hypothetically?", "question_pos_tagged": ["RB", "."], "aSentId": 457, "answer": "I don't do hypothetical, Piers.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "VB", "PRP", ":", "VB", "PRP", "VB", "IN", "DT", ":"], "qSentId": 472, "question": "Would you -- would you join the Tea Party?", "question_pos_tagged": ["MD", "PRP", ":", "MD", "PRP", "VB", "DT", "NNP", "NNP", "."], "aSentId": 473, "answer": "Well, let me -- let me finish with the --", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["NNP", ",", "EX", "VBZ", "DT", "JJ", "NN", "NN", "EX", "."], "qSentId": 483, "question": "Because if -- if your party gets split by her and the Tea Party, isn't that a big problem for you?", "question_pos_tagged": ["IN", "IN", ":", "IN", "PRP$", "NN", "NNS", "NN", "IN", "PRP$", "CC", "DT", "NNP", "NNP", ",", "VBZ", "RB", "IN", "DT", "JJ", "NN", "IN", "PRP", "."], "aSentId": 484, "answer": "Well, there's a long logic chain there.", "question_ne_tagged": {"ORGANIZATION": ["Tea Party"]}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", ":", "PRP", "VBP", "DT", ":", "PRP", "VBP", "JJ", "NNS", "."], "qSentId": 487, "question": "Could you win with her do you think?", "question_pos_tagged": ["MD", "PRP", "VB", "IN", "PRP$", "VBP", "PRP", "VB", "."], "aSentId": 488, "answer": "I -- I have no -- I do international politics.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["PRP", ":", "PRP", "VBP", "RB", "JJ", "IN", "DT", ":", "DT", "NNS", "WDT", "VBP", "IN", "DT", "NN", "."], "qSentId": 498, "question": "-- you're not fearful of that candidate being Sarah Palin?", "question_pos_tagged": [":", "PRP", "VBP", "RB", "JJ", "IN", "DT", "NN", "VBG", "NNP", "NNP", "."], "aSentId": 499, "answer": "I -- I'm not fearful of the -- the candidates that are on the horizon.", "question_ne_tagged": {"PERSON": ["Sarah Palin"]}}, {"answer_ne_tagged": {}, "docId": "110119pmt", "answer_pos_tagged": ["IN", "NN", "PRP", "MD", "."], "qSentId": 513, "questi