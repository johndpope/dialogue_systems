: ["NNP", "."], "qSentId": 590, "question": "Am I right?", "question_pos_tagged": ["NNP", "PRP", "RB", "."], "aSentId": 591, "answer": "Yeah.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110601pmt", "answer_pos_tagged": ["NNP", ",", "RB", "JJ", "IN", "NN", "."], "qSentId": 600, "question": "What did you mean by that?", "question_pos_tagged": ["WP", "VBD", "PRP", "VB", "IN", "DT", "."], "aSentId": 601, "answer": "Well, partly tongue in cheek.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110601pmt", "answer_pos_tagged": ["PRP", "VBD", "PRP", ",", "CD", ",", "PRP", "VBP", "PRP", "."], "qSentId": 611, "question": "Why do you keep doing it?", "question_pos_tagged": ["WRB", "VBP", "PRP", "VB", "VBG", "PRP", "."], "aSentId": 612, "answer": "I told you, one, I enjoy it.", "question_ne_tagged": {}}, {"answer_ne_tagged": {}, "docId": "110601pmt", "answer_pos_tagged": ["NNP", "."], "qSentId": 614, "question": "Snobbery?", "question_pos_tagged": ["NNP", "."], "aSentId": 615, "answer": "Yeah.", "question_ne_tagged": {}}]